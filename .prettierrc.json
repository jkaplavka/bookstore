{
    "tabWidth": 4,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "twigPrintWidth": 120,
    "twigAlwaysBreakObjects": false,
    "twigOutputEndblockName": false,
    "twigMultiTags": [
        "nav,endnav",
        "switch,case,default,endswitch",
        "ifchildren,endifchildren",
        "cache,endcache",
        "js,endjs"
    ],
    "plugins": ["./node_modules/prettier-plugin-twig-melody"]
}
